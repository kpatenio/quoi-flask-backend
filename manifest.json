{
  "name": "QUOI",
  "version": "1.0",
  "description":"The Chrome extension to use when you keeping asking yourself « Quoi ? »",
  "manifest_version":2,
  "background":{
    "scripts":["jquery-3.3.1.min.js", "background.js"],
    "persistent":false
  },
  "browser_action":{
    "default_popup":"popup.html",
    "default_icon":{
      "16":"images/fleur-128.png",
      "32":"images/fleur-128.png",
      "48":"images/fleur-128.png",
      "148":"images/fleur-128.png"
    }
  },
  "icons":{
    "16":"images/fleur-128.png",
    "128":"images/fleur-128.png"
  },
  "permissions":["storage", "contextMenus", "tabs"]
}
